<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>OAuthImplement.home</servlet-class>

    </servlet>

    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AuthCodeURL</servlet-name>
        <servlet-class>OAuthImplement.AuthCodeURL</servlet-class>

    </servlet>

    <servlet-mapping>
        <servlet-name>AuthCodeURL</servlet-name>
        <url-pattern>/AuthCodeURL</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>OAuthClient</servlet-name>
        <servlet-class>OAuthImplement.OAuthClient</servlet-class>

    </servlet>

    <servlet-mapping>
        <servlet-name>OAuthClient</servlet-name>
        <url-pattern>OAuthClient</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TokenCodeURL</servlet-name>
        <servlet-class>OAuthImplement.TokenCodeURL</servlet-class>

    </servlet>

    <servlet-mapping>
        <servlet-name>TokenCodeURL</servlet-name>
        <url-pattern>/TokenCodeURL</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TokenInfo</servlet-name>
        <servlet-class>OAuthImplement.TokenInfo</servlet-class>

    </servlet>

    <servlet-mapping>
        <servlet-name>TokenInfo</servlet-name>
        <url-pattern>/TokenInfo</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>JSON</servlet-name>
        <servlet-class>OAuthImplement.JSON</servlet-class>

    </servlet>

    <servlet-mapping>
        <servlet-name>JSON</servlet-name>
        <url-pattern>/JSON</url-pattern>
    </servlet-mapping>



</web-app>
